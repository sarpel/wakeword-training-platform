# ANTHROPIC AUTH CONFIGURATION FOR Z.AI
# SECURITY: Replace YOUR_TOKEN_HERE with your actual token

# Windows PowerShell
$env:ANTHROPIC_BASE_URL="https://api.z.ai/api/anthropic"
$env:ANTHROPIC_AUTH_TOKEN="YOUR_TOKEN_HERE"
$env:ANTHROPIC_MODEL="GLM-4.6"
$env:ANTHROPIC_SMALL_FAST_MODEL="GLM-4.6"

# Linux terminal
export ANTHROPIC_BASE_URL="https://api.z.ai/api/anthropic"      
export ANTHROPIC_AUTH_TOKEN="YOUR_TOKEN_HERE"
export ANTHROPIC_MODEL="GLM-4.6"
export ANTHROPIC_SMALL_FAST_MODEL="GLM-4.6"